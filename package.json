{
  "name": "rn-obfuscating-transformer",
  "version": "1.0.1",
  "description": "Obfuscating transformer for react-native",
  "main": "dist/index.js",
  "repository": "https://github.com/TheLonelyAstronaut/react-native-obfuscating-transformer",
  "author": "David Sheldrick",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.21.5",
    "@babel/parser": "^7.21.5",
    "@babel/traverse": "^7.21.5",
    "@types/app-root-path": "^1.2.5",
    "@types/node": "^16",
    "@types/semver": "^7.5.0",
    "app-root-path": "^3.1.0",
    "javascript-obfuscator": "^4.0.2",
    "jju": "^1.4.0",
    "metro-bundler": "^0.22.1",
    "semver": "^7.5.1",
    "source-map": "^0.6.1"
  },
  "peerDependencies": {
    "react-native": ">=0.45.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "precommit": "tsc && lint-staged",
    "test": "jest"
  },
  "typings": "dist/index.d.ts",
  "lint-staged": {
    "**/*.ts": [
      "prettier --no-semi --trailing-comma all"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "husky": "^0.13.3",
    "jest": "^29.5.0",
    "lint-staged": "^3.4.1",
    "metro": "^0.76.4",
    "metro-react-native-babel-preset": "0.73.5",
    "np": "^4.0.2",
    "prettier": "^2.4.1",
    "react-native": "^0.71.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(javascript-obfuscator)/)"
    ],
    "moduleNameMapper": {
      "javascript-obfuscator": "<rootDir>/node_modules/javascript-obfuscator/index.ts"
    },
    "testRegex": "src/.*(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
